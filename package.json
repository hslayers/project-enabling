{
  "name": "hslayers-project-enabling",
  "private": false,
  "version": "0.5.5",
  "description": "HSLayers-NG Enabling project",
  "repository": "git+ssh://git@github.com:hslayers/project-enabling.git",
  "license": "MIT",
  "scripts": {
    "postinstall": "",
    "prestart": "npm install",
    "start": "http-server -a localhost -p 8000",
    "build": "node node_modules/hslayers-ng/scripts/bootstrap-isolate.js && node_modules/.bin/webpack --config bestPractices/src/webpack.prod.js",
    "build-dev": "node node_modules/hslayers-ng/scripts/bootstrap-isolate.js && node_modules/.bin/webpack --config bestPractices/src/webpack.dev.js --progress --watch",
    "pretest": "npm install",
    "test": "karma start test/karma.conf.js",
    "test-single-run": "karma start test/karma.conf.js  --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor test/protractor-conf.js"
  },
  "dependencies": {
    "hslayers-ng": "^1.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-angularjs-annotate": "^0.10.0",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^3.5.1",
    "extract-loader": "^5.0.1",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^3.2.0",
    "ng-cache-loader": "0.0.26",
    "postcss": "^7.0.27",
    "postcss-prefix-selector": "^1.7.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
